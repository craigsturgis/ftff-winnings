<!DOCTYPE html>
<html>
	<head>
		<title>Passport-Yahoo (OAuth) Example</title>
	</head>
	<body>
		<% if (!user) { %>
			<p>
			<a href="<%= root %>/">Home</a> |
			<a href="<%= root %>/login">Log In</a>
			</p>
		<% } else { %>
			<p>
			<a href="<%= root %>/">Home</a> |
			<a href="<%= root %>/account">Account</a> |
			<a href="<%= root %>/logout">Log Out</a>
			</p>
		<% } %>
		<%- body %>
	</body>
</html>
